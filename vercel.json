{
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/cron/fetch-sources",
      "schedule": "*/30 * * * *"
    },
    {
      "path": "/api/cron/morning-digest",
      "schedule": "0 11 * * *"
    },
    {
      "path": "/api/cron/evening-digest",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/weekly-digest",
      "schedule": "0 13 * * 0"
    }
  ]
}
